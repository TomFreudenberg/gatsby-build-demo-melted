{
  "name": "gatsby-starter-hello-world",
  "private": true,
  "description": "A simplified bare-bones starter for Gatsby",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "build": "npm run build:pdf && npm run clean && npm run build:app",
    "build:app": "cross-env BUILD_APP=true gatsby build",
    "build:pdf": "cross-env BUILD_PDF=true gatsby build",
    "postbuild:pdf": "find ./public -type f -path \"*/pdf_*/*\" -name \"*.html\" -exec node bin/puppeteer-to-pdf.js \"{}\" \"src/pdf_documents\" \\;",
    "develop": "gatsby develop",
    "format": "prettier --write src/**/*.{js,jsx}",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\""
  },
  "dependencies": {
    "cross-env": "^5.2.0",
    "gatsby": "^2.1.27",
    "gatsby-plugin-sass": "^2.0.1",
    "gatsby-plugin-manifest": "^2.0.0",
    "gatsby-plugin-offline": "^2.0.0",
    "gatsby-source-filesystem": "^2.0.23",
    "node-sass": "^4.9.0",
    "puppeteer": "^1.12.2",
    "react": "^16.8.4",
    "react-dom": "^16.8.4"
  },
  "devDependencies": {
    "prettier": "^1.16.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-hello-world"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  }
}
